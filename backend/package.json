{
  "name": "cnd-uniformes-backend",
  "version": "1.0.0",
  "description": "Backend pour CND Uniformes",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "migrate:dev": "NODE_ENV=development npx knex migrate:latest --knexfile knexfile.js",
    "rollback:dev": "NODE_ENV=development npx knex migrate:rollback --knexfile knexfile.js",
    "seed:dev": "NODE_ENV=development npx knex seed:run --knexfile knexfile.js",
    "migrate:prod": "NODE_ENV=production npx knex migrate:latest --knexfile knexfile.js",
    "rollback:prod": "NODE_ENV=production npx knex migrate:rollback --knexfile knexfile.js",
    "seed:prod": "NODE_ENV=production npx knex seed:run --knexfile knexfile.js"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.3",
    "qs": "^6.14.0",
    "sqlite3": "^5.1.6",
    "stripe": "^14.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
